#include <iostream>
#include <string>
#include <sstream>
#include <fstream>
using namespace std;
ofstream f("C:\\Users\\79892\\Desktop\\dz.txt");

void Razm(string strok, int ur, int n, int k)
{
    int p = 0, t = 0, y = 0, o = 0, u = 0, yy = 0, x = 0;
    if (ur > k)
    {

        for (int i = 0; i < k; i++)
        {
            if (strok[i] == '1')
                p++;
            if (strok[i] == '2')
                t++;;
            if (strok[i] == '3')
                y++;
            if (strok[i] == '4')
                o++;
            if (strok[i] == '5')
                u++;
            if (strok[i] == '6')
                yy++;
        }
        if( (p ==1&&t==1&&y==1&&o==1&&u>=2&&yy>=2)||
            (p == 1 && t == 1 && y == 1 && o >= 2 && u >= 2 && yy ==1) ||
            (p == 1 && t == 1 && y >=2 && o == 1 && u >= 2 && yy ==1) ||
            (p == 1 && t >= 2 && y == 1 && o == 1 && u >= 2 && yy==1) ||
            (p>=2 && t == 1 && y == 1 && o == 1 && u >= 2 && yy ==1))
        {
            for (int i = 0; i < k; i++)
            {
                if (strok[i] == '0')
                    strok[i] = 'a';
                if (strok[i] == '1')
                    strok[i] = 'b';
                if (strok[i] == '2')
                    strok[i] = 'c';
                if (strok[i] == '3')
                    strok[i] = 'd';
                if (strok[i] == '4')
                    strok[i] = 'e';
                if (strok[i] == '5')
                    strok[i] = 'f';
            }
            f << strok << endl;
        }
    }
    else
        for (int i = 0; i < n; i++)
        {
            ostringstream os;
            os << i+1 ;

            Razm(strok + os.str(), ur + 1, n, k);
        }
}
int main()
{

    int n;
    int k, tt;
    setlocale(LC_ALL, "Russian_Russia.1251");
    cout << "введите n и k: " << endl;
    cin >> n >> k;
    Razm("", 1, n, k);
        cout<<"Готово";
    return 0;
}
